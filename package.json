{
  "name": "jumpsuit",
  "version": "0.0.3",
  "description": "One framework to rule them all",
  "homepage": "http://jumpsuit.io",
  "license": "MIT",
  "keywords": [
    "framework",
    "react",
    "redux",
    "react-router",
    "spa",
    "front-end"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpsuit/jumpsuit.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib/",
    "README.md"
  ],
  "scripts": {
    "lint": "exlint src",
    "build": "rm -rf lib && babel src -d lib",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build -- --compact --no-comments"
  },
  "dependencies": {
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-actions": "0.9.1",
    "redux-devtools": "3.2.0",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-thunk": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "exlint": "^0.1.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
