{
  "name": "jumpsuit",
  "version": "0.6.3",
  "description": "One javascript framework to rule them all",
  "homepage": "http://jumpsuit.io",
  "license": "MIT",
  "keywords": [
    "framework",
    "react",
    "redux",
    "react-router",
    "spa",
    "front-end"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpsuit/jumpsuit.git"
  },
  "main": "lib/core/index.js",
  "bin": {
    "jumpsuit": "bin/jumpsuit"
  },
  "files": [
    "jumpsuit.js",
    "bin/",
    "lib/",
    "examples/",
    "logo.txt",
    "banner.png",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test:lint": "standard --verbose | snazzy",
    "test:unit": "nyc ava test/cli/unit test/core/unit -a",
    "test:integration": "ava test/cli/integration test/core/integration",
    "pretest": "rm -rf .nyc_output coverage",
    "test": "cross-env NODE_ENV=testing run-s test:*",
    "posttest": "nyc report -r=lcov",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "prebuild": "rm -rf lib && rm -rf jumpsuit.js",
    "build": "babel src -d lib --no-comments",
    "watch": "npm run build -- -w -s",
    "standalone": "browserify lib/core/index.js -s jumpsuit -tg uglifyify -o jumpsuit.js",
    "prepublish": "npm run build && npm run standalone"
  },
  "dependencies": {
    "aliasify": "^2.0.0",
    "axios": "0.11.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-runtime": "6.6.1",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "browserify-transform-tools": "1.6.0",
    "butler": "0.4.1",
    "chalk": "1.1.3",
    "chokidar": "1.4.3",
    "clean-css": "3.4.12",
    "crypto-extra": "0.2.2",
    "exorcist": "^0.4.0",
    "forgetify": "0.1.2",
    "fs-extra": "0.30.0",
    "fs-promise": "0.5.0",
    "glob": "7.0.3",
    "insert-css": "0.2.0",
    "lodash": "4.12.0",
    "loose-envify": "1.1.0",
    "memory-fs": "0.3.0",
    "minimatch": "3.0.0",
    "minimist": "1.2.0",
    "postcss": "5.0.21",
    "postcss-import": "8.1.2",
    "postcss-modules": "0.4.1",
    "query-string": "4.1.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-thunk": "2.0.1",
    "resolve": "1.1.7",
    "strip-ansi": "3.0.1",
    "uglify-js": "2.6.2",
    "uglifyify": "3.0.1",
    "vinyl-transform": "^1.0.0"
  },
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "^6.1.1",
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.8.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "cross-spawn": "^4.0.0",
    "npm-run-all": "2.1.1",
    "nyc": "6.4.4",
    "proxyquire": "1.7.8",
    "sinon": "1.17.4",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src"
    ]
  }
}
