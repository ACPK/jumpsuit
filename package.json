{
  "name": "jumpsuit",
  "version": "0.0.4",
  "description": "one framework to rule them all",
  "homepage": "http://jumpsuit.io",
  "license": "MIT",
  "keywords": [
    "framework",
    "react",
    "redux",
    "react-router",
    "spa",
    "front-end"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpsuit/jumpsuit.git"
  },
  "main": "lib/core/index.js",
  "bin": {
    "jumpsuit": "bin/jumpsuit"
  },
  "files": [
    "bin/",
    "lib/",
    "README.md"
  ],
  "scripts": {
    "lint": "exlint src test bin",
    "pretest": "rm -rf .nyc_output coverage && npm run lint",
    "test": "cross-env NODE_ENV=testing nyc ava -a",
    "posttest": "nyc report -r=lcov",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "build": "rm -rf lib && babel src -d lib",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build -- --compact --no-comments"
  },
  "dependencies": {
    "babel-core": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-runtime": "6.6.1",
    "babelify": "7.3.0",
    "browserify": "13.0.0",
    "chalk": "1.1.3",
    "chokidar": "1.4.3",
    "connect": "3.4.1",
    "fs-extra": "0.30.0",
    "fs-promise": "0.5.0",
    "loose-envify": "1.1.0",
    "minimist": "1.2.0",
    "nib": "1.1.0",
    "query-string": "4.1.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-actions": "0.9.1",
    "redux-devtools": "3.2.0",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-thunk": "2.0.1",
    "rememberify": "1.0.1",
    "serve-static": "1.10.2",
    "stylus": "0.54.5",
    "uglify-js": "2.6.2",
    "uglifyify": "3.0.1",
    "websocket": "1.0.22"
  },
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "6.8.0",
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.8.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "exlint": "0.1.4",
    "nyc": "6.4.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src"
    ]
  }
}
